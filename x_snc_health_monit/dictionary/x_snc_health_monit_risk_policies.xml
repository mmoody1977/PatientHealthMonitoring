<?xml version="1.0" encoding="UTF-8"?><database>
    <element label="Risk Policies" max_length="40" name="x_snc_health_monit_risk_policies" sizeclass="8" type="collection">
        <element label="Active" max_length="40" name="active" type="boolean"/>
        <element dependent="table" dependent_on_field="table" label="Conditions" max_length="4000" name="conditions" type="conditions" use_dependent_field="true"/>
        <element attributes="edge_encryption_enabled=true" label="Name" max_length="40" name="name" type="string"/>
        <element label="Order" max_length="40" name="order" type="integer"/>
        <element attributes="edge_encryption_enabled=true" choice="1" label="Policy Type" max_length="40" name="policy_type" type="string">
            <choice>
                <element inactive_on_update="false" label="COVID RISK" sequence="0" value="COVID RISK"/>
                <element inactive_on_update="false" label="Health Risk" sequence="1" value="Health Risk"/>
            </choice>
        </element>
        <element dependent="target_table" dependent_on_field="target_table" label="Risk Field" max_length="80" name="risk_field" type="field_name" use_dependent_field="true"/>
        <element attributes="edge_encryption_enabled=true" choice="1" label="Risk Value" max_length="40" name="risk_value" type="string">
            <choice>
                <element inactive_on_update="false" label="High" value="High"/>
                <element inactive_on_update="false" label="Low" value="Low"/>
                <element inactive_on_update="false" label="Medium" value="Medium"/>
            </choice>
        </element>
        <element attributes="base_start=true" label="Table" max_length="80" name="table" type="table_name"/>
        <element attributes="base_start=true" label="Target Table" max_length="80" name="target_table" type="table_name"/>
    </element>
</database>
