        <audit>false</audit>
        <calculation><![CDATA[(function calculatedFieldValue(current) {

    var codes = "";
    var encQuery = "patient=" + current.sys_id;
    var grRec = new GlideRecord('x_snc_health_monit_patient_contitions');
    grRec.addEncodedQuery(encQuery);
    grRec.query();
    while (grRec.next()) {
        if (codes == '') {

            codes = grRec.code.sys_id;
        } else {
            codes += "," + grRec.code.sys_id;
        }

    }


    var encRiskQuery = "risk_type=Mortality^risk_for=fdc3f4b91b68d0100cd4c91b1e4bcbd3^risk_from.sys_idIN" + codes;
    var maxRisk = '';
    var grRiskAgg = new GlideAggregate('x_snc_health_monit_risk_analysis');
    grRiskAgg.addEncodedQuery(encRiskQuery);
    grRiskAgg.addAggregate('MAX', 'risk_percentage');
    grRiskAgg.groupBy('risk_type');
    grRiskAgg.query();
    if (grRiskAgg.next()) {

        maxRisk = grRiskAgg.getAggregate('MAX', 'risk_percentage');
        if (maxRisk != 0) {
			 return maxRisk;
        } else {
              return '';
        }
    } else {
        return '';
    }


})(current);]]></calculation>
